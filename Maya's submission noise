def MayaStitForTwotatswithendmoveone(mymoves, othermoves, totalRounds, currentRound):
    #Stays silent until the other player rats twice in a row. If the other player's last 2 moves is rat this player only rats for this round.
    if currentRound == 49:
        return True
    if len(othermoves) < 2:
        return False
    if othermoves[-1] and othermoves[-2]:
        return True
    else:
        return False

